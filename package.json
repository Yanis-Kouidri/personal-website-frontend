{
  "name": "portfolio-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc --noEmit && vite build",
    "check": "tsc --noEmit",
    "check:strict": "tsc --noEmit --strict",
    "lint": "biome check .",
    "lint:fix": "biome check --write . ",
    "format": "biome format .",
    "format:fix": "biome format --write . ",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --write --no-errors-on-unmatched",
      "vitest related --run"
    ]
  },
  "dependencies": {
    "@types/qs": "^6.14.0",
    "axios": "^1.13.2",
    "lucide-react": "^0.562.0",
    "qs": "^6.14.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "styled-components": "^6.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "retire": "^5.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
